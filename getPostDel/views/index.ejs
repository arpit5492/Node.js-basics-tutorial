<html lang="en">
<%- include("./partials/head.ejs") %>
<body>
  <%- include("./partials/nav.ejs") %>
  <div class="blogs content">
    <h2>All blogs</h2>
    <% if (b.length > 0) { %>
      <% b.forEach(blog => { %>
        <a class="single" href="/All-blogs/<%= blog._id %>">
          <h3><%= blog.title %></h3>
          <p><%= blog.snippet %></p>
        </a>
      <% }); %>
    <% } else { %>
      <p>There are no blogs to display...</p>
    <% } %>
  </div>
  <%- include("./partials/footer.ejs")%>
  
</body>
</html>